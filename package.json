  "author": "Ilsur Gabdulkhakov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/li4e/just-react-tooltip/issues"
  },
  "homepage": "https://github.com/li4e/just-react-tooltip#readme",
